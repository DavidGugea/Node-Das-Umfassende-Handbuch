{
    "extends": ["airbnb", "prettier"],
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier": 
        [
            "warn",
            {
                "singleQuote": true,
                "endOfLine": "auto",
                "tabWidth": 4
            }
        ],
        "no-unused-vars":"warn",
        "no-console":"off",
        "func-names":"off",
        "prefer-arrow-callback":"off"
    }
}
